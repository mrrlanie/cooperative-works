#include <stdio.h>
#include <stdlib.h>
#include <conio.h>
#include <time.h>

int main(){
    int mass[2][16] = {0};
/////////////////индексы картинок от 1 до 64//////////////////////
    for (int i = 1; i < 2; i++){
        for (int j = 0; j < 16; j++){
            mass[i][j] = mass[i][j-1] + 1;
        }
    }

//////////////////комбинации//////////////////////////////
    for (int i = 0; i < 16; i++){
        int counter = i;
        char bit[4] = "0000";
        for (int j = 0; j < 4; j++){
            if (counter > 0){
                bit[j] = (counter % 2 == 0) ? '0' : '1';
                counter = counter / 2;
            }
            else break;
        }
        for (int j = 0; j < 2; j++){
            char temp = bit[j];
            bit[j] = bit[3 - j];
            bit[3 - j] = temp;
        }
        mass[0][i] = atoi(bit);
    }

    int user_data = 0;
    int randomize_number = 0;
    scanf("%d", &user_data);

/////////////////////Сравнение введенных ответов с комбинацией////////////////////////////
    for (int j = 0;j < 16; j++){
        if (user_data == mass[0][j]){
            randomize_number = j;
            break;
        }
    }

////////////////Рандомайзер///////////////////////////
    srand(time(NULL));
    int random = 1 + rand() % 2;
    int output = mass[random][randomize_number]; //Номер картинки

    printf("%d",output);

    return  0;
}
